#bai 4,5 
def bai4():
  b = []
  for i in range(1,21):
    b.append(i)
  print(b)
bai4()

def bai5():
  b=[]
  for i in range(1,21):
    if i % 3 == 0: 
      b.append(i)
  print(b)
bai5()


