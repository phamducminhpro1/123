#bai 8 
a = [1, 5, -9, 3]
def xoaheadortail(m):
  if m == 'head':
    return a.remove(a[0])
  else:
    return a.remove(a[-1])
xoaheadortail('head')
print(a)



#bai 9 
import cmath
def quadro(a, b, c):
  if a == 0:
    return -c/b
  elif b*b - 4*a*c < 0:
    return "khong co nghiem"
  elif b*b - 4*a*c == 0:
    return -b/(2*a)
  else:
    return [(-b-cmath.sqrt(b*b - 4*a*c))/(2*a), (-b+ cmath.sqrt(b*b - 4*a*c))/(2*a)]
print(quadro(1,-5,4))
print(quadro(1,4,4)
